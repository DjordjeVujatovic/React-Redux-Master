{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "es6": true
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "max-len": 1,
        "no-param-reassign": 1,
        "react/jsx-filename-extension": 0,
        "global-require": 0,
        "no-case-declarations": 0,
        "react/prefer-stateless-function": 1,
        "react/jsx-no-bind": 0,
        "react/forbid-prop-types": 0,
        "no-underscore-dangle": 0
    }
}